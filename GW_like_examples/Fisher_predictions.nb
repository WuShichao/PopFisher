Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalGamma]\[Lambda]\[Lambda]", "=", 
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"\[Theta]min", " ", "=", 
         SuperscriptBox["10", "4"]}], " ", ",", 
        RowBox[{"\[Theta]max", " ", "=", " ", 
         SuperscriptBox["10", "7"]}], ",", 
        RowBox[{"\[CapitalGamma]", " ", "=", " ", "\[CapitalGamma]"}], ",", 
        " ", 
        RowBox[{"H", " ", "=", " ", "H"}], ",", " ", 
        RowBox[{"D2", " ", "=", " ", "Dmij"}], ",", 
        RowBox[{"D1", "=", "Dmi"}]}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"model", " ", "=", " ", 
        RowBox[{"ppop", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"\[Theta]", ",", "\[Theta]min", ",", "\[Theta]max"}], 
           "}"}], ",", "\[Lambda]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"a", " ", "=", " ", 
        RowBox[{"Integrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            FractionBox[
             RowBox[{"pdet\[Theta]", "[", "\[Theta]", "]"}], 
             RowBox[{"model", " ", 
              RowBox[{"pdet\[Lambda]", "[", "\[Lambda]", "]"}]}]], ")"}], 
           RowBox[{"D", "[", 
            RowBox[{"model", ",", "\[Lambda]"}], "]"}], "*", 
           RowBox[{"D", "[", 
            RowBox[{"model", ",", "\[Lambda]"}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"\[Theta]", ",", "\[Theta]min", ",", "\[Theta]max"}], 
           "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
