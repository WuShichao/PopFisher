Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalGamma]\[Lambda]\[Lambda]", "=", 
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"\[Theta]min", " ", "=", 
         SuperscriptBox["10", "4"]}], " ", ",", 
        RowBox[{"\[Theta]max", " ", "=", " ", 
         SuperscriptBox["10", "7"]}], ",", 
        RowBox[{"\[CapitalGamma]", " ", "=", " ", "\[CapitalGamma]"}], ",", 
        " ", 
        RowBox[{"H", " ", "=", " ", "H"}], ",", " ", 
        RowBox[{"D2", " ", "=", " ", "Dmij"}], ",", 
        RowBox[{"D1", "=", "Dmi"}]}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"model", " ", "=", " ", 
        RowBox[{"ppop", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"\[Theta]", ",", "\[Theta]min", ",", "\[Theta]max"}], 
           "}"}], ",", "\[Lambda]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"a", " ", "=", " ", 
        RowBox[{"Integrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            FractionBox[
             RowBox[{"pdet\[Theta]", "[", "\[Theta]", "]"}], 
             RowBox[{"model", " ", 
              RowBox[{"pdet\[Lambda]", "[", "\[Lambda]", "]"}]}]], ")"}], 
           RowBox[{"D", "[", 
            RowBox[{"model", ",", "\[Lambda]"}], "]"}], "*", 
           RowBox[{"D", "[", 
            RowBox[{"model", ",", "\[Lambda]"}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"\[Theta]", ",", "\[Theta]min", ",", "\[Theta]max"}], 
           "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"b", " ", "=", " ", 
        RowBox[{
         RowBox[{"-", " ", 
          FractionBox["1", 
           SuperscriptBox[
            RowBox[{"pdet\[Lambda]", "[", "\[Lambda]", "]"}], "2"]]}], 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"pdet\[Lambda]", "[", "\[Lambda]", "]"}], ",", 
           "\[Lambda]"}], "]"}], "*", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"pdet\[Lambda]", "[", "\[Lambda]", "]"}], ",", 
           "\[Lambda]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"c", " ", "=", " ", 
        RowBox[{
         FractionBox["1", "2"], " ", 
         RowBox[{"Integrate", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"D", "[", 
             RowBox[{
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{"Log", "@", 
                 RowBox[{"(", 
                  RowBox[{"\[CapitalGamma]", "+", "H"}], ")"}]}], ",", 
                "\[Lambda]"}], "]"}], ",", "\[Lambda]"}], "]"}], 
            FractionBox[
             RowBox[{"pdet\[Theta]", "[", "\[Theta]", "]"}], 
             RowBox[{"pdet\[Lambda]", "[", "\[Lambda]", "]"}]], "model"}], 
           ",", 
           RowBox[{"{", 
            RowBox[{"\[Theta]", ",", "\[Theta]min", ",", "\[Theta]max"}], 
            "}"}]}], "]"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"d", " ", "=", 
        RowBox[{
         RowBox[{"-", 
          FractionBox["1", "2"]}], " ", 
         RowBox[{"Integrate", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"D", "[", 
             RowBox[{
              RowBox[{"D", "[", 
               RowBox[{
                SuperscriptBox[
                 RowBox[{"(", 
                  RowBox[{"\[CapitalGamma]", "+", "H"}], ")"}], 
                 RowBox[{"-", "1"}]], ",", "\[Lambda]"}], "]"}], ",", 
              "\[Lambda]"}], "]"}], "D2", 
            FractionBox["model", 
             RowBox[{"pdet\[Lambda]", "[", "\[Lambda]", "]"}]]}], ",", 
           RowBox[{"{", 
            RowBox[{"\[Theta]", ",", "\[Theta]min", ",", "\[Theta]max"}], 
            "}"}]}], "]"}]}]}], ";", "\[IndentingNewLine]", 
